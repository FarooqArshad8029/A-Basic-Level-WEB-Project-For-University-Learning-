                  <form action="/signUpUser" method="post">
                    <h2 class="text-left mb-4">Sign Up</h2>
                    <div class="form-group text-left">
                      <label class="">First Name</label>
                      <input
                        name="firstName"
                        id="FirstName"
                        class="form-control rounded-11"
