    <!---script code for  signup data  --->

    <script type="text/javascript">
      const checkValidation = () => {
        const firstName = document.getElementById("FirstName").value;
        const lastName = document.getElementById("LastName").value;
        const emailAddress = document.getElementById("EmailAddress").value;
        const phoneNo = document.getElementById("PhoneNo").value;
        const password = document.getElementById("Password").value;
        const verifypassword = document.getElementById("VerifyPassword").value;

        $("#errorFname").css("display", "none");
        $("#errorLname").css("display", "none");
        $("#erroremail").css("display", "none");
        $("#errorphone").css("display", "none");
        $("#errorpassowrd").css("display", "none");
        $("#errorpassword2").css("display", "none");
        $("#errorVpassword").css("display", "none");

        if (firstName.length == 0) {
          $("#errorFname").css("color", "red");
          $("#errorFname").css("display", "block");
          return false;
        } else if (lastName.length == 0) {
          $("#errorLname").css("color", "red");
          $("#errorLname").css("display", "block");
          return false;
        } else if (emailAddress.length == 0) {
          $("#erroremail").css("color", "red");
          $("#erroremail").css("display", "block");
          return false;
        } else if (!emailAddress.includes("@") || !emailAddress.includes(".")) {
          $("#erroremail").text("Please enter a valid email address");
          $("#erroremail").css("color", "red");
          $("#erroremail").css("display", "block");
          return false;
        } else if (phoneNo.length == 0) {
          $("#errorphone").css("color", "red");
          $("#errorphone").css("display", "block");
          return false;
        } else if (password.length == 0) {
          $("#errorpassowrd").css("color", "red");
          $("#errorpassowrd").css("display", "block");
          return false;
        } else if (verifypassword.length == 0) {
          $("#errorVpassword").css("color", "red");
          $("#errorVpassword").css("display", "block");
          return false;
        } else if (password != verifypassword) {
          $("#errorpassword2").css("color", "red");
          $("#errorpassword2").css("display", "block");
          return false;
        } else if (password === verifypassword) {
          let re = /^(?=.*[0-9])(?=.*[!@#$%^&*])[a-zA-Z0-9!@#$%^&*]{6,16}$/;
          if (!re.test(password)) {
            $("#errorpassowrd").css("color", "red");
            $("#errorpassowrd").css("display", "block");
            return false;
          } else {
            signUp();
            return false;
          }
        } else {
          console.log("checkvalidation Else check ");
        }
      };

      const signUp = () => {
        const firstName = document.getElementById("FirstName").value;
        const lastName = document.getElementById("LastName").value;
        const emailAddress = document.getElementById("EmailAddress").value;
        const phoneNo = document.getElementById("PhoneNo").value;
        const password = document.getElementById("Password").value;
        const verifypassword = document.getElementById("VerifyPassword").value;

        const dataobj = {
          firstName,
          lastName,
          emailAddress,
          phoneNo,
          password,
        };

        console.log("dataObj:", dataobj);
      };
    </script>
